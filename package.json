{
  "name": "IBekon-backend",
  "version": "0.0.1",
  "description": "Backend for IBekon",
  "main": "index.js",
  "dependencies": {
    "babel-jest": "^5.3.0",
    "bluebird": "^2.10.2",
    "lite-id": "^1.3.3",
    "lodash": "^3.10.1",
    "stampit": "^2.1.1"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-jest-iojs": "^1.0.3",
    "jsdoc": "^3.3.3"
  },
  "scripts": {
    "test": "jest lib",
    "start": "node index.js",
    "docs": "jsdoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Palid/IBekon-backend.git"
  },
  "author": "Dariusz 'Palid' Niemczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Palid/IBekon-backend/issues"
  },
  "homepage": "https://github.com/Palid/IBekon-backend",
  "engines": {
    "node": "4.1.0"
  },
  "jest": {
    "collectCoverage": true,
    "testDirectoryName": "__tests__",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "lodash",
      "supermixer",
      "stampit",
      "bluebird"
    ]
  }
}
