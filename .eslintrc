{
  "env": {
    "jasmine": true,
    "node": true,
    "mocha": true,
    "browser": false,
    "builtin": true
  },
  "rules": {
    "block-scoped-var": 0,
    "camelcase": 2,
    "curly": [
      0,
      "all"
    ],
    "eqeqeq": [
      2
    ],
    "global-strict": [
      0,
      "never"
    ],
    "guard-for-in": 2,
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-empty": 2,
    "no-extend-native": 2,
    "no-extra-parens": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 0,
    "no-new": 2,
    "no-redeclare": 0,
    "no-sequences": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": true,
    "no-with": 2,
    "quotes": [
      2,
      "single"
    ],
    "strict": 2,
    "valid-typeof": 2,
    "wrap-iife": [
      2,
      "inside"
    ]
  }
}
